{
	"comments": {
		"lineComment": "'",
		"blockComment": [
			"[rR][eE][mM]+",
			"[eE][nN][dD]\\s*[rR][eE][mM]+"
		]
	},
	"brackets": [
		[
			"{",
			"}"
		],
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		[
			"<",
			">"
		]
	],
	"autoClosingPairs": [
		{
			"open": "{",
			"close": "}"
		},
		{
			"open": "[",
			"close": "]"
		},
		{
			"open": "(",
			"close": ")"
		},
		{
			"open": "<",
			"close": ">",
			"notIn": [
				"string",
				"comment"
			]
		},
		{
			"open": "\"",
			"close": "\"",
			"notIn": [
				"string",
				"comment"
			]
		}
	],
	"surroundingPairs": [
		[
			"{",
			"}"
		],
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		[
			"<",
			">"
		],
		[
			"\"",
			"\""
		]
	],
	"folding": {
		"markers": {
			"start": "(?:^|;)\\s*\\b[rR][eE][mM]\\b",
			"end": "(?:^|;)\\s*\\b[eE][nN][dD]\\s?[rR][eE][mM]\\b",
		}
	},
	"indentationRules": {
		"increaseIndentPattern": {
			"pattern": "(?:^|^\\s*(Private|Public)\\s+|;)\\s*\\b(If\\s+|For\\s+|Repeat|While\\s*|Select\\s*|Try\\s*|Case\\s*|Catch\\s*|Function\\s*|Method\\s*|Enum\\s*|Interface\\s*|Type\\s*|Struct\\s*|Private(\\s*|\\s*'.*)$|Else\\s*|ElseIf\\s*)\\b",
			"flags": "i"
		},
		"decreaseIndentPattern": {
			"pattern": "(?:^)\\s*\\b(End\\s*(If|Select|Function|Method|Enum|Interface|Type|Struct|Try|While)\\s*|Next\\s*|Until\\s*|Forever\\s*|Wend\\s*|Public(\\s*|\\s*'.*)$|Case\\s*|Catch\\s*|Else\\s*|ElseIf\\s*)\\b",
			"flags": "i"
		},
		"unIndentedLinePattern": {
			"pattern": "(;\\s*End\\s*\\w+(\\s*|\\s*'.*)$|(?:^|;)\\s*\\bIf\\s+.*(Then\\s+\\w+|\\;\\s*$|(Return.*|Exit|Continue)(\\s*|\\s*'.*)$|\\w\\s+\\w.*\\)(\\s*|\\s*'.*)$)|\\W+\\s+Abstract(\\s+|\\s*$))",
			"flags": "i"
		}
	},
	"onEnterRules": [
		{
			"previousLineText": "(?:^|;)\\s*\\b[fF][uU][nN][cC][tT][iI][oO][nN]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndFunction"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[mM][eE][tT][hH][oO][dD]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndMethod"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[tT][yY][pP][eE]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndType"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[sS][tT][rR][uU][cC][tT]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndStruct"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[eE][nN][uU][mM]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndEnum"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[iI][nN][tT][eE][rR][fF][aA][cC][eE]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndInterface"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[wW][hH][iI][lL][eE]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "Wend"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[fF][oO][rR]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "Next"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[sS][eE][lL][eE][cC][tT]+\\b\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "\tCase \n\tEndSelect"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[rR][eE][mM]\\b",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndRem"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\?\\s*",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "?"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[pP][rR][iI][vV][aA][tT][eE]\\b",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "Public"
			}
		},
		{
			"previousLineText": "(?:^|;)\\s*\\b[iI][fF]\\s+\\w+",
			"beforeText": "(?:^|;)\\s*$",
			"afterText": "^\\s*$",
			"action": {
				"indent": "outdent",
				"appendText": "EndIf"
			}
		}
	]
}